<sut>
	<!-- Example SUT with all remote client parameters 
	-->	
	<remoteclient_1>
		<class>com.aqua.filetransfer.ftp.FTPRemoteClient</class>
		<host>192.12.34.56</host>
		<protocol>telnet</protocol>
		<port>23</port>
		<user>admin</user>
		<password>admin</password>
		<ftpServerHostName>12.11.23.44</ftpServerHostName>
		<ftpUserName>aqua</ftpUserName>
		<ftpPassword>aqua</ftpPassword>
	</remoteclient_1>
	
	<!-- 
	  Typical SUT
	  Opens telnet connection to the remote machine	  
	-->	
	<remoteclient_2>
		<class>com.aqua.filetransfer.ftp.FTPRemoteClient</class>
		<host>192.12.34.56</host>
		<user>admin</user>
		<password>admin</password>
	</remoteclient_2>
</sut>
